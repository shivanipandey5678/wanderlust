<% if(success && success.length){ %>
    <div class="alert alert-success col-lg-6 col-sm-12 col-md-10 col-10 mx-auto  alert-dismissible fade show" role="alert">
       <%=success%>
       <button type="button" class="btn-close btn-sm btn-sm-lg" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
<%}%>


<% if (error && error.length) { %>
    <div class="alert alert-danger col-lg-6 col-sm-12 col-md-10 col-10 mx-auto  alert-dismissible fade show" role="alert">
        <%=error%>
        <button type="button" class="btn-close btn-sm btn-sm-lg" data-bs-dismiss="alert" aria-label="Close"></button>
       </div>
<% } %>
